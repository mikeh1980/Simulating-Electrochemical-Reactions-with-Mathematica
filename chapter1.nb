     FractionBox[
      RowBox[{"f", "(", "0", ")"}], 
      RowBox[{"d", " ", "t"}]]}], "-", 
    RowBox[{
     SuperscriptBox["s", 
      RowBox[{"n", "-", "3"}]], 
