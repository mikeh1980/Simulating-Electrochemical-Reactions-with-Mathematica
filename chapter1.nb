Cell[TextData[{
 "To solve problems using Laplace transforms with Mathematica users of \
versions of ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " versions below 4.0 first need to load the package ",
 StyleBox["LaplaceTransform.m", "MB"],
 " which is located in the ",
 StyleBox["Calculus", "File"],
 " subdirectory of the ",
 StyleBox["StandardPackages", "File"],
 " subdirectory in the directory ",
 StyleBox["AddOns", "File"],
 ". To get version 4.0 and higher to run identically to version 3.0 execute \
the work around in the cell below at the beginning of all notebooks which use \
Laplace transform. This was written by Lou D\[CloseCurlyQuote]Andria of \
Wolfram."
}], "Text",ExpressionUUID->"0c1a3ce0-f8a2-4aa7-956c-f642dc176ac6"],

Cell[BoxData[
 RowBox[{
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"$VersionNumber", ">", "3."}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Unprotect", "[", "LaplaceTransform", "]"}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"LaplaceTransform", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"Derivative", "[", "d__", "]"}], "[", "c", "]"}], "[", 
         "args__", "]"}], ",", "t_", ",", "s_"}], "]"}], ":=", 
      RowBox[{"With", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"pos", "=", 
          RowBox[{"Position", "[", 
           RowBox[{
            RowBox[{"{", "args", "}"}], ",", "t"}], "]"}]}], "}"}], ",", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Derivative", "[", 
            RowBox[{"Sequence", "@@", 
             RowBox[{"Delete", "[", 
              RowBox[{
               RowBox[{"{", "d", "}"}], ",", 
               RowBox[{"pos", "\[LeftDoubleBracket]", 
                RowBox[{"1", ",", "1"}], "\[RightDoubleBracket]"}]}], "]"}]}],
             "]"}], "[", 
           RowBox[{"C", "[", "s", "]"}], "]"}], "[", 
          RowBox[{"Sequence", "@@", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Delete", "[", 
              RowBox[{
               RowBox[{"{", "args", "}"}], ",", 
               RowBox[{"pos", "\[LeftDoubleBracket]", 
                RowBox[{"1", ",", "1"}], "\[RightDoubleBracket]"}]}], "]"}], "/.", 
             RowBox[{"t", "\[Rule]", "s"}]}], ")"}]}], "]"}], "/;", 
