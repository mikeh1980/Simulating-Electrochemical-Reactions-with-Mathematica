Cell[TextData[{
 "The inverse transform of the product of two transformed functions ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "(", "s", ")"}], TraditionalForm]],ExpressionUUID->
  "d6faa7af-179e-48cf-9b14-0c6a6062e698"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"g", "(", "s", ")"}], TraditionalForm]],ExpressionUUID->
  "da988b82-d3fe-49ee-94b4-5e784ed746c8"],
 " can be obtained by using the convolution theorem. The version of the \
convolution integral applicable to electrochemistry requires that for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "<", "0"}], TraditionalForm]],ExpressionUUID->
  "c65d9b2d-f54a-426c-9a06-fc063f918634"],
 " both ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "0", ")"}], "=", "0"}], TraditionalForm]],
  ExpressionUUID->"1de1a790-dad8-414c-b7a8-ba81e7000062"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"g", "(", "0", ")"}], "=", "0"}], TraditionalForm]],
  ExpressionUUID->"57d2a3e4-e3bc-489e-895b-a0f1dba023a8"],
 ". The convolution theorem is:"
}], "Text",ExpressionUUID->"fb06595c-d046-47df-8437-95363d181eed"],

Cell[TextData[Cell[BoxData[{
 FormBox[
  RowBox[{
   RowBox[{
    SuperscriptBox["\[ScriptCapitalL]", 
     RowBox[{"-", "1"}]], 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       OverscriptBox["f", "-"], "(", "s", ")"}], 
      RowBox[{
       OverscriptBox["g", "-"], "(", "s", ")"}]}], "}"}]}], "=", 
   "\[AlignmentMarker]", 
   RowBox[{
    SubsuperscriptBox["\[Integral]", "0", "t"], 
    RowBox[{
     RowBox[{"f", "(", 
      RowBox[{"t", "-", "z"}], ")"}], 
     RowBox[{"g", "(", "z", ")"}], 
     RowBox[{"\[DifferentialD]", "z"}]}]}]}], 
  TraditionalForm], "\[IndentingNewLine]", 
 FormBox[
  RowBox[{"=", "\[AlignmentMarker]", 
   RowBox[{
    SubsuperscriptBox["\[Integral]", "0", "t"], 
    RowBox[{
     RowBox[{"f", "(", "z", ")"}], 
     RowBox[{"g", "(", 
      RowBox[{"t", "-", "z"}], ")"}], 
