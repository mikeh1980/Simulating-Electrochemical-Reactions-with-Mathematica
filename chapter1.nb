    FractionBox[
     RowBox[{
      SuperscriptBox["d", 
       RowBox[{"n", "-", "1"}]], 
      RowBox[{"f", "(", "0", ")"}]}], 
     RowBox[{"d", " ", 
      SuperscriptBox["t", 
       RowBox[{"n", "-", "1"}]]}]]}]}], 
  TraditionalForm]],ExpressionUUID->"d93f3c2d-4bb2-41ae-9956-636625f83bc0"]], \
