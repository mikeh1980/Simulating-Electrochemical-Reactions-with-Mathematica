        SuperscriptBox["2", 
         RowBox[{"n", "-", "1"}]]},
       {"\[VerticalEllipsis]", "\[CenterEllipsis]", "\[VerticalEllipsis]", 
        "\[DescendingEllipsis]", "\[VerticalEllipsis]"},
       {"1", 
        RowBox[{"(", 
         RowBox[{"n", "-", "1"}], ")"}], 
        SuperscriptBox[
