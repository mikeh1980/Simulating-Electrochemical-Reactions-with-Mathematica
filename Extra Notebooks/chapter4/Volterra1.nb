Cell[CellGroupData[{

Cell["Procedural Implementation of Huber's Method", "Subsection",
 PageWidth->
  WindowWidth,ExpressionUUID->"8f632e63-acbd-4ab2-90b0-dbd8179b7f09"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Clear", "[", 
    RowBox[{"d", ",", "n", ",", "g", ",", "b", ",", "f", ",", "m"}], "]"}], 
   ";"}], "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{"d", "=", "0.2"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"n", "=", "100"}], ";"}], "\n", 
 RowBox[{"g", ":=", 
  RowBox[{"0.75", "/", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{"Exp", "[", 
        RowBox[{"10.", "-", 
         RowBox[{"m", "*", "d"}]}], "]"}]}], ")"}], "*", 
     SuperscriptBox["d", 
      RowBox[{"3", "/", "2"}]]}], ")"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"a", "=", 
   RowBox[{"ConstantArray", "[", 
    RowBox[{"0.", ",", 
     RowBox[{"{", "n", "}"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"f", "=", 
   RowBox[{"ConstantArray", "[", 
    RowBox[{"0.", ",", 
     RowBox[{"{", "n", "}"}]}], "]"}]}], ";"}]}], "InputOnly",
 PageWidth->WindowWidth,
 CellLabel->"In[14]:=",ExpressionUUID->"740e20e5-33c4-478c-83fb-8ffaff462440"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"m", "=", "1"}], ",", 
    RowBox[{"m", "<", 
     RowBox[{"n", "+", "1"}]}], ",", 
    RowBox[{"m", "++"}], ",", 
    RowBox[{
     RowBox[{"a", "\[LeftDoubleBracket]", "m", "\[RightDoubleBracket]"}], "=", 
     RowBox[{"g", "-", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"i", "=", "1"}], 
        RowBox[{"m", "-", "1"}]], 
       RowBox[{
        RowBox[{"a", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
        "*", 
        RowBox[{"(", 
         RowBox[{
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"m", "-", "i", "+", "1"}], ")"}], 
           RowBox[{"3", "/", "2"}]], "-", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"m", "-", "i"}], ")"}], 
           RowBox[{"3", "/", "2"}]]}], ")"}]}]}]}]}]}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], "=", 
   RowBox[{"(", 
    RowBox[{"d", "*", 
     RowBox[{"a", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}], 
    ")"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"For", "[", 
  RowBox[{
   RowBox[{"m", "=", "2"}], ",", 
   RowBox[{"m", "<", 
    RowBox[{"n", "+", "1"}]}], ",", 
   RowBox[{"m", "++"}], ",", 
   RowBox[{
    RowBox[{"f", "\[LeftDoubleBracket]", "m", "\[RightDoubleBracket]"}], "=", 
    
    RowBox[{
     RowBox[{"(", 
      RowBox[{"d", "*", 
       RowBox[{"a", "\[LeftDoubleBracket]", "m", "\[RightDoubleBracket]"}]}], 
      ")"}], "+", 
     RowBox[{"f", "\[LeftDoubleBracket]", 
      RowBox[{"m", "-", "1"}], "\[RightDoubleBracket]"}]}]}]}], "]"}], "\n", 
 RowBox[{
  RowBox[{"result", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"10", "-", 
        RowBox[{"(", 
         RowBox[{"m", "*", "d"}], ")"}]}], ",", 
       RowBox[{
        RowBox[{"-", 
         RowBox[{"Sqrt", "[", "Pi", "]"}]}], "*", 
        RowBox[{
        "f", "\[LeftDoubleBracket]", "m", "\[RightDoubleBracket]"}]}]}], 
      "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"m", ",", "1", ",", "n"}], "}"}]}], "]"}]}], 
  ";"}]}], "InputOnly",
 PageWidth->WindowWidth,
 CellLabel->"In[20]:=",ExpressionUUID->"f78d3698-c24f-48a5-a4c3-7bc7e62aec9e"],

