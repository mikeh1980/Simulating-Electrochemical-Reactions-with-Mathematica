Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"ClearAll", "[", 
    RowBox[{
    "n", ",", "\[Upsilon]", ",", "ks", ",", "ki", ",", "\[CapitalDelta]e", 
     ",", "d", ",", "g", ",", "h1", ",", "a", ",", "\[Chi]"}], "]"}], ";"}], 
  "\n"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"n", "=", "200"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Upsilon]", "=", "1."}], ";"}], 
  RowBox[{"(*", 
   RowBox[{"sweep", " ", "rate"}], "*)"}]}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"ks", "=", "1.*^-6"}], ";"}], 
  RowBox[{"(*", 
   RowBox[{"standard", " ", "rate", " ", "constant"}], "*)"}]}], "\n", 
 RowBox[{
  RowBox[{"ki", "=", 
   RowBox[{"ks", "*", 
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", "\[Alpha]"}], "*", "10."}], "]"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"\[CapitalDelta]e", "=", "0.25"}], ";"}], 
  RowBox[{"(*", 
   RowBox[{"dimensionless", " ", "potential", " ", "increment"}], 
   "*)"}]}], "\n", 
 RowBox[{
  RowBox[{"d", "=", 
   RowBox[{"\[Alpha]", "*", "\[CapitalDelta]e"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"g", "[", "m_", "]"}], ":=", 
   RowBox[{"d", "*", 
    RowBox[{"Sqrt", "[", 
     RowBox[{"Pi", "*", "\[Alpha]", "*", 
      FractionBox["F", 
       RowBox[{"R", "*", "T"}]], "*", "\[Upsilon]", "*", 
      "\[ScriptCapitalD]"}], "]"}], "*", 
    RowBox[{
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"-", "m"}], "*", "d"}], "]"}], "/", "ki"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"h1", "=", 
   RowBox[{"0.75", "*", 
    SuperscriptBox["d", 
     RowBox[{"3", "/", "2"}]]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"a", "=", 
   RowBox[{"ConstantArray", "[", 
    RowBox[{"0.", ",", "n"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"\[Chi]", "=", 
   RowBox[{"ConstantArray", "[", 
    RowBox[{"0.", ",", "n"}], "]"}]}], ";"}]}], "InputOnly",
 CellLabel->"In[12]:=",ExpressionUUID->"ee39404a-fec2-4b96-9cf1-81060ff25291"],

