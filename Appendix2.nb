  RowBox[{"semiIntD2", "[", 
   RowBox[{"list_List", ",", "\[CapitalDelta]\[Tau]_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"y1", ",", "y2", ",", "y3", ",", "col1", ",", "col2"}], "}"}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     StyleBox[
      RowBox[{"separate", " ", "the", " ", "two", " ", "columns"}], "Remark"],
      " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"col1", "=", 
      RowBox[{"Drop", "[", 
       RowBox[{
        RowBox[{"list", "\[LeftDoubleBracket]", 
         RowBox[{"All", ",", "1"}], "\[RightDoubleBracket]"}], ",", 
        RowBox[{"-", "1"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"col2", "=", 
      RowBox[{"list", "\[LeftDoubleBracket]", 
       RowBox[{"All", ",", "2"}], "\[RightDoubleBracket]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"y1", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"1", "/", 
         SqrtBox[
          RowBox[{
           RowBox[{"Length", "[", "col2", "]"}], "-", "i", "+", "0.5"}]]}], 
        ",", " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", " ", 
          RowBox[{"Length", "[", "col2", "]"}], ",", "2", ",", 
          RowBox[{"-", "1"}]}], "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"y2", "=", 
      RowBox[{"ListCorrelate", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"1", ",", "1"}], "}"}], ",", "col2"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"y3", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"0.5", "*", 
