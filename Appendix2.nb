Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"X", "(", 
    RowBox[{"k", "\[CapitalDelta]\[Tau]"}], ")"}], "=", 
   RowBox[{
    FractionBox["1", 
     SuperscriptBox["\[Pi]", 
      RowBox[{"1", "/", "2"}]]], 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"j", "=", "1"}], "k"], 
     FractionBox[
      RowBox[{
       RowBox[{"\[Chi]", "(", 
        RowBox[{
         RowBox[{"j", "\[CapitalDelta]\[Tau]"}], "-", 
         RowBox[{
          FractionBox["1", "2"], "\[CapitalDelta]\[Tau]"}]}], ")"}], 
       SuperscriptBox["\[CapitalDelta]\[Tau]", 
        RowBox[{"1", "/", "2"}]]}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"k", "-", "j", "+", 
         FractionBox["1", "2"]}], ")"}], 
       RowBox[{"1", "/", "2"}]]]}]}]}], 
  TraditionalForm]],ExpressionUUID->"2f7eaa2d-4e75-4086-9300-94ec4f343bb8"]], \
"AppendixEquation",
 CellTags->"tag112",ExpressionUUID->"9fa1f307-a62c-42de-b7dc-cd2c5f4b6e12"],

