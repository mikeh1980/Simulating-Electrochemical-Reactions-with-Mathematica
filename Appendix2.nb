}], "Text",ExpressionUUID->"6ad96130-645e-4e6d-a410-2e6db15f113c"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Unprotect", "[", "D", "]"}], ";"}], "\n", 
 RowBox[{}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"D", "[", 
   RowBox[{
    SuperscriptBox["x_", "m_"], ",", 
    RowBox[{"{", 
     RowBox[{"x_", ",", "q_"}], "}"}]}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    FractionBox[
     RowBox[{"Gamma", "[", 
      RowBox[{"m", "+", "1"}], "]"}], 
     RowBox[{"Gamma", "[", 
      RowBox[{"m", "-", "q", "+", "1"}], "]"}]], 
    SuperscriptBox["x", 
     RowBox[{"m", "-", "q"}]]}], "/;", 
   RowBox[{
    RowBox[{
     RowBox[{"Head", "[", "q", "]"}], "\[Equal]", " ", "Real"}], " ", "||", 
    " ", 
    RowBox[{
     RowBox[{"Head", "[", "q", "]"}], "\[Equal]", " ", "Rational"}], "||", 
    RowBox[{
     RowBox[{"Head", "[", "q", "]"}], "\[Equal]", " ", 
     "Complex"}]}]}]}], "\n", 
 RowBox[{}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Protect", "[", "D", "]"}], ";"}]}], "InputOnly",
 CellLabel->"In[41]:=",ExpressionUUID->"c968eaaa-13ee-4730-9700-19e41893b10f"],

