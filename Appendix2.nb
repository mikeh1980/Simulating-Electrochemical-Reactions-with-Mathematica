}], "Text",ExpressionUUID->"9dab403c-7324-4034-910b-9629c2a093b2"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"ClearAll", "[", "semiIntD", "]"}], ";"}], "\n", 
 RowBox[{}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"semiIntD", "[", 
    RowBox[{"list_List", ",", "\[CapitalDelta]\[Tau]_"}], "]"}], ":=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"list", "\[LeftDoubleBracket]", 
        RowBox[{"k", ",", "1"}], "\[RightDoubleBracket]"}], ",", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"0.5", "*", 
          SqrtBox[
           FractionBox[
            RowBox[{"Abs", "[", "\[CapitalDelta]\[Tau]", "]"}], "\[Pi]"]]}], 
         ")"}], "*", 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"i", "=", "2"}], "k"], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"list", " ", "\[LeftDoubleBracket]", " ", 
              RowBox[{
               RowBox[{"i", "-", "1"}], ",", "2"}], "\[RightDoubleBracket]"}],
              "+", 
             RowBox[{"list", "\[LeftDoubleBracket]", 
              RowBox[{"i", ",", "2"}], "\[RightDoubleBracket]"}]}], ")"}], 
           "/", 
           SqrtBox[
            RowBox[{"(", 
             RowBox[{"k", "-", "i", "+", "0.5"}], ")"}]]}], ")"}]}]}]}], 
      "}"}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"k", ",", " ", "2", ",", " ", 
       RowBox[{"Length", "[", "list", "]"}]}], "}"}]}], "]"}]}], 
  ";"}]}], "InputOnly",
 CellLabel->"In[19]:=",ExpressionUUID->"91adb7d3-3b3e-4f9a-bf58-9c84256299f1"],

