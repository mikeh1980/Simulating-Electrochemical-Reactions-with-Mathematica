 RowBox[{}], "\n", 
 RowBox[{
  RowBox[{"d", "=", "0.2"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"n", "=", "100"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"g", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"0.75", "/", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "+", 
          RowBox[{"Exp", "[", 
           RowBox[{"10", "-", 
            RowBox[{"i", "*", "d"}]}], "]"}]}], ")"}], "*", 
        SuperscriptBox["d", 
         RowBox[{"3", "/", "2"}]]}], ")"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "n"}], "}"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"bottom", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"n", "-", "i", "+", "1"}], ")"}], 
         RowBox[{"3", "/", "2"}]], "-", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"n", "-", "i"}], ")"}], 
         RowBox[{"3", "/", "2"}]]}], ")"}], "//", "N"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "n", ",", "1", ",", 
       RowBox[{"-", "1"}]}], "}"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"x", "[", 
    RowBox[{"r_", ",", "s_"}], "]"}], ":=", 
   RowBox[{"bottom", "\[LeftDoubleBracket]", 
    RowBox[{"r", "-", "s", "+", "1"}], "\[RightDoubleBracket]"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{"mat", "=", 
   RowBox[{"LowerDiagonalMatrix", "[", 
    RowBox[{"x", ",", "n"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"a", "=", 
   RowBox[{"LinearSolve", "[", 
    RowBox[{"mat", ",", "g"}], "]"}]}], ";"}], "\n", 
 RowBox[{}], "\n", 
 RowBox[{
  RowBox[{"f", "=", 
