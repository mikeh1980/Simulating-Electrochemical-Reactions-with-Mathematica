Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{"d", ",", "n", ",", "g", ",", "a", ",", "f", ",", "m"}], "]"}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{"d", "=", "0.2"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"n", "=", "100"}], ";"}], "\n", 
 RowBox[{"g", ":=", 
  RowBox[{"0.75", "/", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{"Exp", "[", 
        RowBox[{"10.", "-", 
         RowBox[{"m", "*", "d"}]}], "]"}]}], ")"}], "*", 
     SuperscriptBox["d", 
      RowBox[{"3", "/", "2"}]]}], ")"}]}]}], "\n", 
 RowBox[{
  RowBox[{"a", "=", 
   RowBox[{"ConstantArray", "[", 
    RowBox[{"0.", ",", 
     RowBox[{"{", "n", "}"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"f", "=", 
   RowBox[{"ConstantArray", "[", 
    RowBox[{"0.", ",", 
     RowBox[{"{", "n", "}"}]}], "]"}]}], ";"}]}], "InputOnly",
 CellLabel->"In[58]:=",ExpressionUUID->"405df6bd-83aa-417b-ad85-c162a42e1587"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "next", " ", "step", " ", "evaluate", " ", "is", " ", "to", " ", 
    "\"\<a\>\""}], "*)"}], "\n", 
  RowBox[{
   RowBox[{"For", "[", 
    RowBox[{
     RowBox[{"m", "=", "1"}], ",", 
     RowBox[{"m", "<", 
      RowBox[{"n", "+", "1"}]}], ",", 
     RowBox[{"m", "++"}], ",", 
     RowBox[{
      RowBox[{"a", "\[LeftDoubleBracket]", "m", "\[RightDoubleBracket]"}], 
      "=", 
      RowBox[{"g", "-", 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"i", "=", "1"}], 
         RowBox[{"m", "-", "1"}]], 
        RowBox[{
         RowBox[{"a", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
         "*", 
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"m", "-", "i", "+", "1"}], ")"}], 
            RowBox[{"3", "/", "2"}]], "-", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"m", "-", "i"}], ")"}], 
            RowBox[{"3", "/", "2"}]]}], ")"}]}]}]}]}]}], "]"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"then", " ", "evaluate", " ", "\"\<f\>\""}], "*)"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"f", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], "=", 
     RowBox[{"(", 
      RowBox[{"d", "*", 
       RowBox[{"a", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}], 
      ")"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"For", "[", 
    RowBox[{
     RowBox[{"m", "=", "2"}], ",", 
     RowBox[{"m", "<", 
      RowBox[{"n", "+", "1"}]}], ",", 
     RowBox[{"m", "++"}], ",", 
     RowBox[{
      RowBox[{"f", "\[LeftDoubleBracket]", "m", "\[RightDoubleBracket]"}], 
      "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"d", "*", 
         RowBox[{
         "a", "\[LeftDoubleBracket]", "m", "\[RightDoubleBracket]"}]}], ")"}],
        "+", 
       RowBox[{"f", "\[LeftDoubleBracket]", 
        RowBox[{"m", "-", "1"}], "\[RightDoubleBracket]"}]}]}]}], "]"}], "\n", 
   RowBox[{
    RowBox[{"result", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"10", "-", 
          RowBox[{"(", 
           RowBox[{"m", "*", "d"}], ")"}]}], ",", 
         RowBox[{
          RowBox[{"-", 
           RowBox[{"Sqrt", "[", "Pi", "]"}]}], "*", 
          RowBox[{
          "f", "\[LeftDoubleBracket]", "m", "\[RightDoubleBracket]"}]}]}], 
        "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"m", ",", "1", ",", "n"}], "}"}]}], "]"}]}], 
    ";"}]}]}]], "InputOnly",
 CellLabel->"In[64]:=",ExpressionUUID->"083195eb-b7c2-4ce8-b714-7d90bd630be7"],

