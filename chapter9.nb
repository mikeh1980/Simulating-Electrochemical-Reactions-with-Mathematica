   RowBox[{"standard", " ", "rate", " ", "constant"}], "*)"}]}], "\n", 
 RowBox[{
  RowBox[{"n", "=", "401"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"\[DoubleStruckCapitalD]", "=", "0.35"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"\[Alpha]", "=", ".5"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"\[ScriptCapitalD]", "=", "1.*^-5"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"iDim", "=", 
   FractionBox[
